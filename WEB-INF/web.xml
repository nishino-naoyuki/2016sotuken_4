<?xml version="1.0" encoding="UTF-8" ?>
<web-app>

  <filter>
    <filter-name>EncodingFilter</filter-name>
    <filter-class>keiziban.filter.EncodingFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>EncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>


<servlet>
	<servlet-name>TopAction</servlet-name>
	<servlet-class>keiziban.action.TopAction</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>TopAction</servlet-name>
	<url-pattern>/top</url-pattern>
</servlet-mapping>


<servlet>
	<servlet-name>MapHinanjo</servlet-name>
	<servlet-class>keiziban.action.MapHinanjo</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>MapHinanjo</servlet-name>
	<url-pattern>/maphinanjo</url-pattern>
</servlet-mapping>


<servlet>
	<servlet-name>KeizibanHinanjo</servlet-name>
	<servlet-class>keiziban.action.KeizibanHinanjo</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>KeizibanHinanjo</servlet-name>
	<url-pattern>/keizibanhinanjo</url-pattern>
</servlet-mapping>

</web-app>